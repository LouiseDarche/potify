<div class="container">
  <h1>Welcome to plants index</h1>
  <%= render "shared/search" %>
  <div class="row">
    <% @plants.each do |plant| %>
      <div class="col-xs-12 col-md-4">
        <%= link_to plant_path(plant) do %>
          <%= render "shared/card", element: plant %>
        <% end %>
      </div>
    <% end %>
  </div>
</div>
